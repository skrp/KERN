# INTRO #################
  SOTOPSK = usb -> host$usr_s
  ROTOPSK = usb -> host$usr_r
  
  usr -> usb -> host$usr_s
  
  $ip4{$port} = $usr_key
  
  fred -> SOTOPSK -> 11101@0.6.6.6:6663
  fred <- 11101@0.6.6.6:3666 <-ROTOPSK_PACKET <- service
